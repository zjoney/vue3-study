#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zhangjing/Desktop/persional/zhufeng-study/vue3-study/Vue3-component 1/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/zhangjing/Desktop/persional/zhufeng-study/vue3-study/Vue3-component 1/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/vue-tsc@0.34.17_typescript@4.7.4/node_modules/vue-tsc/bin/vue-tsc.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/vue-tsc@0.34.17_typescript@4.7.4/node_modules/vue-tsc/bin/vue-tsc.js" "$@"
fi
